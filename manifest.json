{
    "name": "Dictionary",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "dictionarylogo.png",
        "default_title": "Find the meaning of a word!",
        "default_popup": "popup.html"
    },
    "background":{
        "scripts": ["background.js"]
    }
}